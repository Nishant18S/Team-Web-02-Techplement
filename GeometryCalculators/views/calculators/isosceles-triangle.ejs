<!-- Isosceles Triangle Calculator -->
<div id="isosceles-triangle">
    <h2>Isosceles Triangle Calculator</h2>
    <form data-type="isosceles-triangle" onsubmit="event.preventDefault(); calculate('isosceles-triangle')">
        <input type="number" name="base" placeholder="Base">
        <input type="number" name="side" placeholder="Height">
        <button type="submit">Calculate</button>
    </form>
    <p id="isosceles-triangle-result"></p>
</div>
