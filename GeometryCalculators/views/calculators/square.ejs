<!-- Square Calculator -->
<div class="calculator-card">
    <h2>Square Calculator</h2>
    <form data-type="square" onsubmit="event.preventDefault(); calculate('square')">
        <div class="input-group">
            <input type="number" name="side" placeholder="Side">
        </div>
        <button type="submit" class="calculate-btn">Calculate</button>
        <div id="square-result" class="result-text"></div>
    </form>
    
</div>