<!-- Equilateral Triangle Calculator -->
<div id="equilateral-triangle">
    <h2>Equilateral Triangle Calculator</h2>
    <form data-type="equilateral-triangle" onsubmit="event.preventDefault(); calculate('equilateral-triangle')">
        <input type="number" name="side" placeholder="Side Length">
        <button type="submit">Calculate</button>
    </form>
    <p id="equilateral-triangle-result"></p>
</div>
