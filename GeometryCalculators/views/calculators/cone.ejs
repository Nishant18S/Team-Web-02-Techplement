<!-- Cone Calculator -->
<div id="cone">
    <h2>Cone Calculator</h2>
    <form data-type="cone" onsubmit="event.preventDefault(); calculate('cone')">
        <input type="number" name="radius" placeholder="Radius">
        <input type="number" name="height" placeholder="Height">
        <button type="submit">Calculate</button>
    </form>
    <p id="cone-result"></p>
</div>
