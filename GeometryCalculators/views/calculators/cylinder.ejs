
<!-- Cylinder Calculator -->
<div id="cylinder">
    <h2>Cylinder Calculator</h2>
    <form data-type="cylinder" onsubmit="event.preventDefault(); calculate('cylinder')">
        <input type="number" name="radius" placeholder="Radius">
        <input type="number" name="height" placeholder="Height">
        <button type="submit">Calculate</button>
    </form>
    <p id="cylinder-result"></p>
</div>